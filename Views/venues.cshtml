@{
	Layout="Shared/_Layout.cshtml";
}

<h1>Venues Catalog</h1>
<hr />
<h2>Venues:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var venue in Model)
    {
      <li><a href='/venue/@venue.Id'>@venue.Id - @venue.Name, @venue.Address</a></li>
			<p><a href="/venue/@venue.Id/update">Update</a> |
			<a href="/venue/@venue.Id/delete">Delete</a></p>
			<br>
    }
  </ul>
}
<h4><a href="/venues/new">Add a new venue</a></h4>
<h4><a href="/">Return home</a></h4>
